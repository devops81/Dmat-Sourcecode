<div  ng-controller="addProfileCtrl"  class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div id="editProfileModal" class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <span class="modal-title headingModalTitle">Edit Profile</span>
            </div>
            <form name="myForm1">
            <div class="modal-body">
                <div class="form-group">
                    <div class="editModalFields">Employee/Guest ID:</div>
                    <input type="text" ng-model="model.modelAddEmpGuestId" class="height34 width-100per" disabled>
                </div>
                <div class="form-group">
                    <div class="editModalFields">Name:</div>
                    <input type="text" ng-model="model.modelProfileName" class="height34 width-100per"
                           name="modelProfileName" ng-minlength="5" ng-maxlength="20" required>

                    <span ng-if="myForm1.modelProfileName.$error.required && myForm1.modelProfileName.$dirty ">Required</span>
                    <!--<span style="color:red;" ng-if="myForm1.modelProfileName.$error.pattern">Alphabets Only hello</span>-->
                    <span style="color:red;" ng-if="myForm1.modelProfileName.$error.minlength || myForm1.modelProfileName.$error.maxlength">
                                Minimum length should be 5 &amp; Maximum should be 20
                    </span>
                </div>
                <div class="form-group">
                    <div class="editModalFields">Role:</div>
                    <select ng-click="test()" ng-model="model.modelProfileRoleId" class="height34 width-100per">
                        <option ng-repeat="role in model.roleList" value="{{role.roleId}}">{{role.role}}</option>
                    </select>
                </div>
                <div class="form-group">
                    <div class="editModalFields">Account:</div>
                    <select ng-model="model.modelProfileAccountId" class="height34 width-100per">
                        <option ng-repeat="account in model.accountList" value="{{account.accountId}}">{{account.account}}</option>
                    </select>
                </div>
                <div class="form-group">
                    <div class="editModalFields">Password:</div>
                    <input type="text" ng-model="model.modelProfilePassword" class="height34 width-100per"
                           name="modelProfilePassword" ng-minlength="5" ng-maxlength="20" required>
                </div>
            </div>

            <div class="modal-footer">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <button ng-click="editProfileDialog.close()" class="addBtnModal btn" ng-disabled="myForm1.$invalid" >
                            Save
                        </button>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <button type="button" class="addBtnModal" data-dismiss="modal">Cancel</button>
                    </div>
                </div>
            </div>
            </form>
        </div>
    </div>
</div>