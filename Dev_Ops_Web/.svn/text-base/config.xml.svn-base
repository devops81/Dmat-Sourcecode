<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.healthappbundle.myHealthApp" version="1.0.0">
  <name>my Health App</name>
  <description>Health App for Patient.</description>
  <author href="http://phonegap.com" email="support@phonegap.com">Innovation Team</author>
  <content src="index.html"/>
  <preference name="permissions" value="none"/>
  <preference name="orientation" value="portrait"/>
  <preference name="target-device" value="universal"/>
  <preference name="fullscreen" value="false"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="prerendered-icon" value="true"/>
  <preference name="stay-in-webview" value="false"/>
  <preference name="ios-statusbarstyle" value="default"/>
  <preference name="detect-data-types" value="true"/>
  <preference name="exit-on-suspend" value="false"/>
  <preference name="show-splash-screen-spinner" value="true"/>
  <preference name="auto-hide-splash-screen" value="true"/>
  <preference name="disable-cursor" value="false"/>
  <preference name="android-minSdkVersion" value="14"/>
  <preference name="android-installLocation" value="auto"/>
  <gap:plugin name="org.apache.cordova.splashscreen"/>   
    
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="5000" />
    
  <icon src="icon.png" gap:role="default"/>

  <icon src="res/icon/android/android_HealthApp_Patient_icon-36.png" gap:platform="android" gap:qualifier="ldpi"/>
  <icon src="res/icon/android/android_HealthApp_Patient_icon-48.png" gap:platform="android" gap:qualifier="mdpi"/>
  <icon src="res/icon/android/android_HealthApp_Patient_icon-72.png" gap:platform="android" gap:qualifier="hdpi"/>
  <icon src="res/icon/android/android_HealthApp_Patient_icon-96.png" gap:platform="android" gap:qualifier="xhdpi"/>

  <gap:splash src="res/screen/android/android_launchScreen_patient_ldpi.png" gap:platform="android" gap:qualifier="port-ldpi"/>
  <gap:splash src="res/screen/android/android_launchScreen_patient_mdpi.png" gap:platform="android" gap:qualifier="port-mdpi"/>
  <gap:splash src="res/screen/android/android_launchScreen_patient_hdpi.png" gap:platform="android" gap:qualifier="port-hdpi"/>
  <gap:splash src="res/screen/android/android_launchScreen_patient_xhdpi.png" gap:platform="android" gap:qualifier="port-xhdpi"/> 
    
  <gap:splash src="res/screen/ios/ios_launchScreen_patient_320x480.png" gap:platform="ios" width="320" height="480"/>
  <gap:splash src="res/screen/ios/ios_launchScreen_patient_640x960.png" gap:platform="ios" width="640" height="960"/>
  <gap:splash src="res/screen/ios/ios_launchScreen_patient_640x1136.png" gap:platform="ios" width="640" height="1136"/>
  <gap:splash src="res/screen/ios/ios_launchScreen_patient_640x1136.png" gap:platform="ios" width="768" height="1024"/>
     
  <gap:splash src="splash.png" />
  
  <access origin="*"/>
  <allow-navigation href="*" />
  <allow-intent href="*" />
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>    
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
     <allow-intent href="sms:*"/>
  </platform>
</widget>